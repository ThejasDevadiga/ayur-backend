title Table 04
meta(charset="utf-8")
meta(name="viewport", content="width=device-width,initial-scale=1,shrink-to-fit=no")
link(href="https://fonts.googleapis.com/css?family=Roboto:400,100,300,700", rel="stylesheet", type="text/css")
link(rel="stylesheet", href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css")
link(rel="stylesheet", href="/css/components/scheduleTable.css")
section.ftco-section
  .container.schedule-table
    .row.justify-content-center
      .col-md-6.text-center.mb-5
        h2.heading-section APPOINTMENTS
    .row
      .col-md-15
        h4.text-center.mb-4 Class Schedule Table
        .table-wrap
          table.table.table-bordered.text-center
            thead
              tr
                th Time
                th Monday
                th Tuesday
                th Wednesday
                th Thursday
                th Friday
                th Saturday
                th Sunday
            tbody
                - var time = 8

                each val,key in appointments
                    tr
                        td #{time}.00-#{time+1}.00
                    
                        each val2,key2 in val 
                            td.text-center
                
                                if val2.status=='1'
                                    .img.rounded-circle.mb-2(style="background-color:green")
                                else
                                    .img.rounded-circle.mb-2(style="background-color:red")
                                a(href="#")
                                    strong #{val2.name}
                                    br
                        - time++


                                
                        
                            

                    

